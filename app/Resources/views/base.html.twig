<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block meta_keywords %}{% endblock %}
        <link rel="stylesheet" href="{{ asset('CSS/general.css') }}">
        <link rel="stylesheet" href="https://bootswatch.com/{% block bs_theme %}cerulean{% endblock %}/bootstrap.min.css">

        <!-- jQuery library -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
        <!-- JavaScript -->
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

        {% block stylesheets %}{% endblock %}
        {% block flvicon %}
        <link rel="icon" type="image/x-icon" href="{{ asset('DefaultImg/Jk.png') }}" />
        {% endblock %}

    </head>
    <body id="body">
        {% block header %} {% endblock %}
        {% block body %}{% endblock %}
        {% block footer %} {% endblock %}
        {% block javascripts %}<script src="{{ asset('js/animation.js') }}"></script>{% endblock %}
    </body>
</html>
